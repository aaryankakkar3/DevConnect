datasource db {
  provider  = "postgresql"
  url       = env("DATABASE_URL")
  directUrl = env("DIRECT_DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id    String     @id @default(uuid())
  email String  @unique
  name  String?
  clearance String
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt @default(now())
}